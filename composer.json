{
    "name": "railt/http",
    "type": "library",
    "description": "The Railt HTTP component provides objects to build GraphQL requests from the client.",
    "license": "MIT",
    "keywords": ["http", "http response", "response", "http request", "request", "graphql"],
    "homepage": "http://railt.org",
    "support": {
        "issues": "https://github.com/railt/railt/issues",
        "source": "https://github.com/railt/http"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": "^7.1.3"
    },
    "autoload": {
        "psr-4": {
            "Railt\\Http\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Tests\\Http\\": "tests"
        }
    },
    "suggest": {
        "symfony/http-foundation": "Symfony HTTP Kernel (>=3.3)",
        "illuminate/http": "Laravel HTTP Kernel (>=5.5)"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
